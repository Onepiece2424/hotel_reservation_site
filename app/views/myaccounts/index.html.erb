<p id="notice"><%= notice %></p>

<h1>プロフィール情報</h1>

<table>
  <thead>
    <tr>
      <th>アイコン</th>
      <th>自己紹介</th>
      <th>名前</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @myaccounts.each do |myaccount| %>
      <tr>
        <td><%= myaccount.icon %></td>
        <td><%= myaccount.introduction %></td>
        <td><%= myaccount.name %></td>
        <td><%= link_to 'Show', myaccount %></td>
        <td><%= link_to 'Edit', edit_myaccount_path(myaccount) %></td>
        <td><%= link_to 'Destroy', myaccount, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Create New Myaccount', new_myaccount_path %>
