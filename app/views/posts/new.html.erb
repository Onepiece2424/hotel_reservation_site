<h3>ルーム情報登録</h3>

 <%= form_for @post do |f| %>
 
  <table>
        <tr>
          <th><%= f.label :roomname ,"ルーム名" %></th>
          <td><%= f.text_field :roomname %></td>
        </tr>
        <tr>
          <th><%= f.label :introduction ,"ルーム紹介" %></th>
          <td><%= f.text_field :introduction %></td>
        </tr>
        <tr>
          <th><%= f.label :price ,"料金" %></th>
          <td><%= f.text_field :price %></td>
        </tr>
        <tr>
          <th><%= f.label :address ,"住所" %></th>
          <td><%= f.text_field :address %></td>
        </tr>
  </table>
  
  　　　<div>
        <ul>
          <li><%= f.submit "登録完了する"%></li>
          <li><%= link_to "TOPページに戻る", :users %></li>
        </ul>
      </div>
 <% end %>